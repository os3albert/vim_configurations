// Paste this {} elements undert the keybindings.json file in vscode to emulate vim environment on exing with alt key
// Place your key bindings in this file to override the defaults
[
    {
        "key": "ctrl+shift+oem_102 ctrl+shift+oem_102",
        "command": "toggleVim"
    },
    {
        "key": "alt+j",
        "command": "vim.remap",
        "when": "inputFocus && vim.mode == 'Insert'",
        "args": {
            "after": [ "Esc", "j" ]
        }
    },
